{
  "name": "decentrilized-protocol",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:wrtc-star": "star-signal --port=9090 --host=127.0.0.1",
    "postinstall": "patch-package",
    "prettier:write": "prettier --parser typescript --write \"src/**/*.{ts,tsx}\"",
    "prettier:staged": "pretty-quick --staged --parser typescript --write",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore src",
    "lint:tests": "eslint --quiet --cache --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore ./src/test",
    "lint:file": "eslint --cache --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "lint:quiet": "eslint --quiet --cache --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --ignore-pattern \"*.test.*\" src",
    "lint:quiet:fix": "eslint --quiet --fix --cache --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --ignore-pattern \"*.test.*\" src",
    "lint:staged": "eslint . --quiet --ignore-path .gitignore --cache --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --ignore-pattern \"*.test.*\""
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:staged",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:staged && lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/chai-subset": "^1.3.3",
    "ajv": "^6.10.2",
    "assert": "^2.0.0",
    "buffer": "^5.4.3",
    "chai-subset": "^1.6.0",
    "date-fns": "^2.8.0",
    "date-fns-tz": "^1.0.8",
    "encoding-down": "^6.3.0",
    "firebase": "^6.4.2",
    "ipfs": "^0.38.1",
    "jest": "^24.2.0-alpha.0",
    "level-js": "^5.0.0",
    "levelup": "^4.3.2",
    "libp2p": "^0.26.2",
    "libp2p-bootstrap": "^0.9.7",
    "libp2p-delegated-content-routing": "^0.3.1",
    "libp2p-delegated-peer-routing": "^0.3.1",
    "libp2p-gossipsub": "0.0.5",
    "libp2p-kad-dht": "^0.16.0",
    "libp2p-spdy": "^0.13.3",
    "libp2p-webrtc-star": "^0.16.1",
    "libp2p-websocket-star": "^0.10.2",
    "localforage": "^1.7.3",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.memoize": "^4.1.2",
    "lz-string": "^1.4.4",
    "normalize-url": "^4.5.0",
    "orbit-db": "^0.22.0",
    "orbit-db-access-controllers": "^0.2.4",
    "orbit-db-cache": "^0.3.0",
    "orbit-db-identity-provider": "^0.2.1",
    "orbit-db-keystore": "^0.3.3",
    "patch-package": "^6.2.0",
    "pull-mplex": "^0.1.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "^3.2.0",
    "scrypt-js": "^3.0.0",
    "sort-keys": "^3.0.0",
    "typescript": "^3.7.2",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@trust/webcrypto": "^0.9.2",
    "@types/assert": "^1.4.3",
    "@types/chai": "^4.2.4",
    "@types/chai-as-promised": "^7.1.2",
    "@types/date-fns": "^2.6.0",
    "@types/encoding-down": "^5.0.0",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^24.9.1",
    "@types/level-js": "^4.0.1",
    "@types/levelup": "^3.1.1",
    "@types/localforage": "0.0.34",
    "@types/lodash.defaultsdeep": "^4.6.6",
    "@types/lodash.memoize": "^4.1.6",
    "@types/lz-string": "^1.3.33",
    "@types/mime": "^2.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "12.0.12",
    "@types/orbit-db": "file:node_modules-local/orbit-db-types",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/uuid": "^3.4.5",
    "@types/validator": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^2.8.1-alpha.12",
    "@typescript-eslint/parser": "^2.8.1-alpha.12",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^25.1.0"
  }
}
